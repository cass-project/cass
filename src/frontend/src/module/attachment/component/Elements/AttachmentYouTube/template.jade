article.component.component-attachment-youtube
  div(*ngIf="isViewMode('feed')")
    cass-attachment-youtube-feed([attachment]="attachment", [view-mode]="viewMode", (open)="open($event)")
  div(*ngIf="isViewMode('grid')")
    cass-attachment-youtube-grid([attachment]="attachment", [view-mode]="viewMode", (open)="open($event)")
  div(*ngIf="isViewMode('list')")
    cass-attachment-youtube-list([attachment]="attachment", [view-mode]="viewMode", (open)="open($event)")
