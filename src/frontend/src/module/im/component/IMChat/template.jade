ul.chatlog
  li(
    *ngFor="#message of messages",
    class="{{message.send_status.code}}"
  )
    img.avatar(src="{{message.author.image.variants['64'].public_path}}")
    .message
      .from {{message.author.greetings.greetings}}
        small &nbsp;{{getDate(message.date_created) }}
      .text {{message.content}}